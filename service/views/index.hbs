      <div class="control-green  color-input grnA" data-color="green" ><input id="green-0"></input><input id="green-1"></input><input id="green-2"></input></div>
      <div class="control-blue   color-input bluA" data-color="blue"  ><input id="blue-0"></input><input id="blue-1"></input><input id="blue-2"></input></div>
      <div class="control-yellow color-input ylwA" data-color="yellow"><input id="yellow-0"></input><input id="yellow-1"></input><input id="yellow-2"></input></div>
      <div class="control-pink   color-input pnkA" data-color="pink"  ><input id="pink-0"></input><input id="pink-1"></input><input id="pink-2"></input></div>
      <div class="control-silver color-input slvA" data-color="silver"><input id="silver-0"></input><input id="silver-1"></input><input id="silver-2"></input></div>
      <div class="control-orange color-input orgA" data-color="orange"><input id="orange-0"></input><input id="orange-1"></input><input id="orange-2"></input></div>
      <div class="control-purple color-input purA" data-color="purple"><input id="purple-0"></input><input id="purple-1"></input><input id="purple-2"></input></div>
      <br>
      <h2>BLACKOUT<h2>
      <div class="control-blackout blkB" data-color="blackout"><input id="blackout-0"></input><input id="blackout-1"></input><input id="blackout-2"></input></div>
  </div>

</div>
