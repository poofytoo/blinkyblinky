<div class="container">
  <div class="controls">
    <h1>2.009blinky <span class="ver">v0.1</span></h1>
    <div>
      <button class="command" data-func="allon">all on</button>
    </div>
    <div>
      <button class="command" data-func="wave">wave</button>
    </div>
    <div>
      <button class="command" data-func="twinkle">twinkle</button>
    </div>
    <div>
      <button class="command" data-func="paparazzi">paparazzi</button>
    </div>
    <!--<div>
      <button class="command" data-func="beacon">beacon</button>
    </div>-->
    <div>
      <button class="command" data-func="propogate">propogate</button>
    </div>
    <div>
      <button class="dim dim-btn" data-func="dim">Dim</button>
    </div>
    <div>
      <button class="fade fade-btn" data-func="fade">Fade Down</button>
    </div>
    <div class="option2 wave-options">
      <h2>Wave Options</h2>
      <button class="run-btn" data-func="wave">START</button>
      <button>Rate <br> <span style="color:white" id="waverate"></span></button>
    </div>
    <div class="option2 twinkle-options">
      <h2>Twinkle Options</h2>
      <button class="run-btn" data-func="twinkle">START</button>
      <button>Brightness <br> <span style="color:white" id="twinkle-brightness"></span></button>
      <button>Rate <br> <span style="color:white" id="twinkle-rate"></span></button>
    </div>
    <div class="option2 paparazzi-options">
      <h2>Paparazzi Options</h2>
      <button class="run-btn" data-func="paparazzi">START</button>
      <button>Probability <br> <span style="color:white" id="prob"></span></button>
    </div>
    <div class="option2 beacon-options">
      <h2>Beacon Options</h2>
      <div class="beacon-text">
        click on seat location to start pulses
      </div>
    </div>
    <div class="option2 propogate-options">
      <h2>Propogate Options</h2>
      <button class="color-btn redA" data-func="propogate" data-color="red">SET</button>
      <button class="color-btn grnA" data-func="propogate" data-color="green">SET</button>
      <button class="color-btn bluA" data-func="propogate" data-color="blue">SET</button>
      <button class="color-btn ylwA" data-func="propogate" data-color="yellow">SET</button>
      <button class="color-btn pnkA" data-func="propogate" data-color="pink">SET</button>
      <button class="color-btn slvA" data-func="propogate" data-color="silver">SET</button>
      <button class="color-btn orgA" data-func="propogate" data-color="orange">SET</button>
      <button class="color-btn purA" data-func="propogate" data-color="purple">SET</button>
    </div>
    <div class="option2 allon-options">
      <h2>All On Options</h2>
      <button class="allon-btn redA" data-func="allon" data-color="red">SET</button>
      <button class="allon-btn grnA" data-func="allon" data-color="green">SET</button>
      <button class="allon-btn bluA" data-func="allon" data-color="blue">SET</button>
      <button class="allon-btn ylwA" data-func="allon" data-color="yellow">SET</button>
      <button class="allon-btn pnkA" data-func="allon" data-color="pink">SET</button>
      <button class="allon-btn slvA" data-func="allon" data-color="silver">SET</button>
      <button class="allon-btn orgA" data-func="allon" data-color="orange">SET</button>
      <button class="allon-btn purA" data-func="allon" data-color="purple">SET</button>
    </div>
    <button class="blackout blackout-btn">Blackout</button>
  </div>
  <div class="map">
  </div>
  <div class="bash">
    <input class="prompt" type="text">
    <div class="response">
    </div>
  </div>
</div>
